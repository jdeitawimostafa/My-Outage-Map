{
  "expo": {
    "name": "my-outage-map",
    "slug": "my-outage-map",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/te-logo-mobile-white.png",
    "backgroundColor":"#205699",
    "scheme": "myoutagemap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#205699"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "ios": {
            "backgroundColor": "#205699",
             "image": "./assets/images/te-logo-mobile-white.png",
            "resizeMode": "cover"
          },
          "android": {
            "backgroundColor": "#205699",
             "image": "./assets/images/te-logo-mobile-white.png",
            "imageWidth": 150
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
